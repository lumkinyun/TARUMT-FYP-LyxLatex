\begin_layout Standard
\begin_inset listings
lstparams "language=Python,breaklines=true"
inline false
status open

\begin_layout Plain Layout

# Program make a simple calculator
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

# This function adds two numbers
\end_layout

\begin_layout Plain Layout

def add(x, y):
\end_layout

\begin_layout Plain Layout

    return x + y
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

# This function subtracts two numbers
\end_layout

\begin_layout Plain Layout

def subtract(x, y):
\end_layout

\begin_layout Plain Layout

    return x - y
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

# This function multiplies two numbers
\end_layout

\begin_layout Plain Layout

def multiply(x, y):
\end_layout

\begin_layout Plain Layout

    return x * y
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

# This function divides two numbers
\end_layout

\begin_layout Plain Layout

def divide(x, y):
\end_layout

\begin_layout Plain Layout

    return x / y
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

print("Select operation.")
\end_layout

\begin_layout Plain Layout

print("1.Add")
\end_layout

\begin_layout Plain Layout

print("2.Subtract")
\end_layout

\begin_layout Plain Layout

print("3.Multiply")
\end_layout

\begin_layout Plain Layout

print("4.Divide")
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

while True:
\end_layout

\begin_layout Plain Layout

    # Take input from the user
\end_layout

\begin_layout Plain Layout

    choice = input("Enter choice(1/2/3/4): ")
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

    # Check if choice is one of the four options
\end_layout

\begin_layout Plain Layout

    if choice in ('1', '2', '3', '4'):
\end_layout

\begin_layout Plain Layout

        num1 = float(input("Enter first number: "))
\end_layout

\begin_layout Plain Layout

        num2 = float(input("Enter second number: "))
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

        if choice == '1':
\end_layout

\begin_layout Plain Layout

            print(num1, "+", num2, "=", add(num1, num2))
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

        elif choice == '2':
\end_layout

\begin_layout Plain Layout

            print(num1, "-", num2, "=", subtract(num1, num2))
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

        elif choice == '3':
\end_layout

\begin_layout Plain Layout

            print(num1, "*", num2, "=", multiply(num1, num2))
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

        elif choice == '4':
\end_layout

\begin_layout Plain Layout

            print(num1, "/", num2, "=", divide(num1, num2))
\end_layout

\begin_layout Plain Layout

        break
\end_layout

\begin_layout Plain Layout

    else:
\end_layout

\begin_layout Plain Layout

        print("Invalid Input")
\end_layout

